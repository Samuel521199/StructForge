# API 配置
# 后端 API 基础地址
VITE_API_BASE_URL=http://localhost:8000/api

# 应用配置
# 应用标题
VITE_APP_TITLE=StructForge

# 应用描述
VITE_APP_DESCRIPTION=AI工作流编排平台

# 环境标识
# development | production | test
VITE_APP_ENV=development

# 是否启用调试模式
VITE_APP_DEBUG=true

# WebSocket 配置
# WebSocket 服务器地址
VITE_WS_URL=ws://localhost:8000/ws

# 功能开关
# 是否启用工作流编辑器
VITE_FEATURE_WORKFLOW_EDITOR=true

# 是否启用 AI 模型管理
VITE_FEATURE_AI_MODELS=true

# 是否启用用户管理
VITE_FEATURE_USER_MANAGEMENT=true

